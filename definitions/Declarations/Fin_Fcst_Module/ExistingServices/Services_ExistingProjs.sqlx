  ,Project_name 
  ,"Audit_Past_NS_EndDt_Has_DefBal" AS Audit
FROM `adswerve-finance-reporting.Fin_Fcst_Module.Services_ExistingProjs` 
WHERE  
  Audit_Past_NS_EndDt = TRUE 
  AND NOT REGEXP_CONTAINS(NS_Project_ID,"Commission|DV360")
  AND (IFNULL(Def_Balance,0) ) <> 0
  

UNION ALL

SELECT 
  NS_Project_ID	
  ,Project_name 
  ,"Audit_Rate_GreaterThan_310" AS Audit
FROM `adswerve-finance-reporting.Fin_Fcst_Module.Services_ExistingProjs` 
WHERE  
  Audit_Rate_GreaterThan_310 = TRUE 
  AND NOT REGEXP_CONTAINS(NS_Project_ID,"Commission|DV360")
  AND (IFNULL(Def_Balance,0) + IFNULL(YTD_Rev,0)) <> 0

UNION ALL

SELECT 
  NS_Project_ID	
  ,Project_name 
  ,"Audit_Rate_LessThan_200" AS Audit
FROM `adswerve-finance-reporting.Fin_Fcst_Module.Services_ExistingProjs` 
WHERE  
  Audit_Rate_LessThan_200 = TRUE
  AND NOT REGEXP_CONTAINS(NS_Project_ID,"Commission|DV360")
  AND (IFNULL(Def_Balance,0) + IFNULL(YTD_Rev,0)) <> 0

UNION ALL

SELECT 
  NS_Project_ID	
  ,Project_name 
  ,"Audit_No_Harvest_ID" AS Audit
FROM `adswerve-finance-reporting.Fin_Fcst_Module.Services_ExistingProjs` 
WHERE  
  Audit_No_Harvest_ID = TRUE 
  AND NOT REGEXP_CONTAINS(NS_Project_ID,"Commission|DV360")
  AND (IFNULL(Def_Balance,0) + IFNULL(YTD_Rev,0)) <> 0

UNION ALL

SELECT 
  NS_Project_ID	
  ,Project_name 
  ,"Audit_Has_UScurrency" AS Audit
FROM `adswerve-finance-reporting.Fin_Fcst_Module.Services_ExistingProjs` 
WHERE  
  Audit_Has_UScurrency = FALSE 
  AND NOT REGEXP_CONTAINS(NS_Project_ID,"Commission|DV360")
  AND (IFNULL(Def_Balance,0) + IFNULL(YTD_Rev,0)) <> 0

UNION ALL

SELECT 
  NS_Project_ID	
  ,Project_name 
  ,"Audit_Has_Project_Type" AS Audit
FROM `adswerve-finance-reporting.Fin_Fcst_Module.Services_ExistingProjs` 
WHERE  
  Audit_Has_Project_Type = FALSE
  AND NOT REGEXP_CONTAINS(NS_Project_ID,"Commission|DV360")
