config {
  type: "operations",
  database: "adswerve-finance-reporting",
  schema: "Fcst_Module",
  name: "Services_ExistingProjs"
}


CREATE OR REPLACE PROCEDURE `adswerve-finance-reporting.Fin_Fcst_Module.Services_ExistingProjects`(Report_Period DATE)
OPTIONS (strict_mode=false)
BEGIN


DECLARE Max_Snapshot,PY_START,CY_START DATE;
-- DECLARE Report_Period DATE;
DECLARE TimePeriod ,T12Total, T24Total,TotalFcst ,SQL1, SQL2, CY, PY, NY STRING;

SET Max_Snapshot = (SELECT MAX(PARSE_DATE('%Y%m%d',E._TABLE_SUFFIX)) FROM `adswerve-finance.Fin_Stage.Stage_SF_EmployeesByAcct__*` E);
-- SET Report_Period = "2025-03-01";
SET CY_START = DATE(EXTRACT(YEAR FROM Report_Period),1,1);
SET PY_START = DATE_SUB(CY_START, INTERVAL 1 YEAR);
SET CY = LEFT(CAST(CY_START AS STRING),4);
SET PY = LEFT(CAST(PY_START AS STRING),4);
SET NY = LEFT(CAST(DATE_ADD(CY_START, INTERVAL 1 YEAR) AS STRING),4);


SET TimePeriod =(
SELECT STRING_AGG(Timeline)
FROM( 
      SELECT CAST( FORMAT_DATE("'%b_%Y'",X) AS STRING) AS Timeline
      FROM UNNEST(
                  GENERATE_DATE_ARRAY(
                                        CAST(CY_START AS DATE) 
                                        ,DATE_ADD(CAST(CY_START AS DATE), INTERVAL 23 MONTH)
                                        ,INTERVAL 1 MONTH)  
                  ) AS X
    )
);


SET T12Total =(
SELECT STRING_AGG(Timeline,'+')
FROM( 
      SELECT CAST( FORMAT_DATE("%b_%Y",X) AS STRING) AS Timeline
      FROM UNNEST(
                  GENERATE_DATE_ARRAY(
                                        CAST(CY_START AS DATE) 
                                        ,DATE_ADD(CAST(CY_START AS DATE), INTERVAL 11 MONTH)
                                        ,INTERVAL 1 MONTH)  
                  ) AS X
    )
);


SET T24Total =(
SELECT STRING_AGG(Timeline,'+')
FROM( 
      SELECT CAST( FORMAT_DATE("%b_%Y",X) AS STRING) AS Timeline
      FROM UNNEST(
                  GENERATE_DATE_ARRAY(
                                        CAST(CY_START AS DATE) 
                                        ,DATE_ADD(CAST(CY_START AS DATE), INTERVAL 23 MONTH)
                                        ,INTERVAL 1 MONTH)  
                  ) AS X
    )
);

SET TotalFcst =(
SELECT STRING_AGG(Timeline,'+')
FROM( 
      SELECT CAST( FORMAT_DATE("%b_%Y",X) AS STRING) AS Timeline
      FROM UNNEST(
                  GENERATE_DATE_ARRAY(
                                        DATE_ADD(CAST(Report_Period AS DATE), INTERVAL 1 MONTH) 
                                        ,DATE_ADD(CAST(CY_START AS DATE), INTERVAL 23 MONTH)
                                        ,INTERVAL 1 MONTH)  
                  ) AS X
    )
);



SET SQL1 = '''
CREATE OR REPLACE TABLE `adswerve-finance-reporting.Fin_Fcst_Module.Services_ExistingProjs` AS (

WITH PVT AS (
SELECT
  * 
FROM ProjectData 
PIVOT(SUM(IFNULL(RevPerMonth,0)) FOR Timeline IN (''' || TimePeriod || ''')))

SELECT
  *
  ,ROUND(SUM(''' || T12Total || '''),2) AS T12Total
  ,ROUND(SUM(''' || T24Total || '''),2) AS T24Total
  ,ROUND(SUM(''' || TotalFcst || '''),2) AS TotalFcst
  ,CASE
    WHEN Project_Type = "Rate Card" THEN 0
    ELSE ROUND(Contract_Val - (IFNULL(RevToDate,0)-IFNULL(Total_Overages,0)) - ROUND(SUM(''' || TotalFcst || '''),2),2)
   END AS Fcst_Var
  ,IF(IN_SF_NotInNS = "F" AND (Class IS NULL OR Class = "" ),TRUE,FALSE) AS Audit_No_Class
  ,IF(IN_SF_NotInNS = "F" AND Project_Type <> "Rate Card" AND (Contract_VAL IS NULL OR Contract_Val=0),TRUE,FALSE) AS Audit_NoContractValue
  ,IF(Infer_StartDt=Infer_EndDt,TRUE,FALSE) AS Audit_StartEnd_Same
  ,IF(IN_SF_NotInNS = "F" AND Inactive ="F" AND NS_EndDt<CAST("''' || Report_Period || '''" AS DATE),TRUE,FALSE) AS Audit_Past_NS_EndDt
  ,IF(IN_SF_NotInNS = "F" AND Contract_Rate>310 AND Inactive="F",TRUE,FALSE) AS Audit_Rate_GreaterThan_310
  ,IF(IN_SF_NotInNS = "F" AND Contract_Rate<200 AND Inactive="F",TRUE,FALSE) AS Audit_Rate_LessThan_200
  ,IF(IN_SF_NotInNS = "F" AND Inactive="F" AND 
      (NS_Harvest_ID IS NULL OR NS_Harvest_ID = ""),TRUE,FALSE) AS Audit_No_Harvest_ID
  ,IF(IN_SF_NotInNS = "F" AND (Currency <> "US Dollar" OR Currency IS NULL),FALSE,TRUE) AS Audit_Has_UScurrency
  ,IF(Project_Type IS NULL OR Project_Type = "",FALSE,TRUE ) AS Audit_Has_Project_Type
FROM PVT
GROUP BY ALL
)
;''';


SET SQL2 = '''
