  AND (IFNULL(Def_Balance,0) + IFNULL(YTD_Rev,0)) <> 0

UNION ALL

SELECT 
  NS_Project_ID	
  ,Project_name 
  ,"Audit_NoContractValue" AS Audit
FROM `adswerve-finance-reporting.Fin_Fcst_Module.Services_ExistingProjs` 
WHERE  
  Audit_NoContractValue = TRUE 
  AND NOT REGEXP_CONTAINS(NS_Project_ID,"Commission|DV360")
  AND (IFNULL(Def_Balance,0) + IFNULL(YTD_Rev,0)) <> 0

UNION ALL

SELECT 
  NS_Project_ID	
  ,Project_name 
  ,"Audit_NoContractValue" AS Audit
FROM `adswerve-finance-reporting.Fin_Fcst_Module.Services_ExistingProjs` 
WHERE  
  Audit_NoContractValue = TRUE 
  AND NOT REGEXP_CONTAINS(NS_Project_ID,"Commission|DV360")
  AND (IFNULL(Def_Balance,0) + IFNULL(YTD_Rev,0)) <> 0

UNION ALL

SELECT 
  NS_Project_ID	
  ,Project_name 
  ,"Audit_StartEnd_Same" AS Audit
FROM `adswerve-finance-reporting.Fin_Fcst_Module.Services_ExistingProjs` 
WHERE  
  Audit_StartEnd_Same = TRUE 
  AND NOT REGEXP_CONTAINS(NS_Project_ID,"Commission|DV360")
  AND (IFNULL(Def_Balance,0) + IFNULL(YTD_Rev,0)) <> 0

